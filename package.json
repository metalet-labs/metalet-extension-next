{
  "name": "metalet",
  "private": true,
  "version": "3.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./vite-config/vite.dev.config.ts",
    "type-check": "vue-tsc --noEmit",
    "clear": "rimraf dist",
    "build:content": "vite build --config ./vite-config/vite.content.config.ts",
    "build:inject": "vite build --config ./vite-config/vite.inject.config.ts",
    "build:injectWatch": "vite build --config ./vite-config/vite.injectwatch.config.ts",
    "build:bg": "vite build --config ./vite-config/vite.background.config.ts",
    "build:web": "vue-tsc && vite build --config ./vite-config/vite.web.config.ts",
    "build": "set NODE_ENV=production && run-s clear build:content build:inject build:bg build:web",
    "bm": "export NODE_ENV=production && export NODE_OPTIONS=--max-old-space-size=6144 && run-s clear build:content build:inject build:bg build:web",
    "b": "yarn build",
    "bw": "yarn build:web",
    "bwc": "yarn build:content --watch",
    "bwi": "yarn build:injectWatch",
    "bwb": "yarn build:bg --watch",
    "bww": "yarn build:web --watch",
    "bwim": "export NODE_OPTIONS=--max-old-space-size=6144 && yarn build:inject --watch",
    "bwbm": "export NODE_OPTIONS=--max-old-space-size=6144 && yarn build:bg --watch",
    "bwww": "set NODE_OPTIONS=--max-old-space-size=4096 && yarn build:web --watch",
    "bwwm": "export NODE_OPTIONS=--max-old-space-size=6144 && yarn build:web --watch"
  },
  "dependencies": {
    "@bitcoinerlab/secp256k1": "^1.1.1",
    "@headlessui/tailwindcss": "^0.1.3",
    "@headlessui/vue": "^1.7.12",
    "@heroicons/vue": "^2.0.16",
    "@metalet/utxo-wallet-service": "0.3.21",
    "@okxweb3/crypto-lib": "^1.0.1",
    "@rollup/plugin-alias": "^4.0.3",
    "@scure/bip39": "^1.2.2",
    "@sentry/vue": "^7.81.1",
    "@tanstack/vue-query": "^4.26.1",
    "@vee-validate/zod": "^4.12.8",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.3.0",
    "@vueuse/integrations": "^10.4.1",
    "@vueuse/router": "^10.11.0",
    "axios": "0.27.2",
    "bignumber.js": "^9.1.2",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "bitcore-lib": "^10.0.0",
    "buffer": "^6.0.3",
    "check-password-strength": "^2.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "create-hash": "^1.2.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "decimal.js": "^10.4.3",
    "ecpair": "^2.1.0",
    "gsap": "^3.12.5",
    "lucide-vue-next": "^0.341.0",
    "memfs": "^4.5.0",
    "meta-contract": "^0.4.4",
    "mvc-std-lib": "^1.1.4",
    "object-hash": "^3.0.0",
    "qrcode": "^1.5.3",
    "radix-vue": "^1.4.9",
    "randombytes": "^2.1.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul-vue": "0.1.0",
    "vee-validate": "^4.12.8",
    "vue": "^3.2.45",
    "vue-i18n": "^10.0.0",
    "vue-router": "^4.1.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
