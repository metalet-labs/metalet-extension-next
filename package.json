    "vite-plugin-wasm": "^3.2.2",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^0.34.3",
    "vue-svg-loader": "^0.16.0",
    "vue-tsc": "^2.1.2",
    "webextension-polyfill": "^0.10.0",
    "yarn": "^1.22.21"
  }
}
