    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.4",
    "vite": "5.4.1",
    "vite-plugin-node-polyfills": "^0.14.1",
    "vite-plugin-svg": "^0.7.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-svgr": "^4.2.0",
